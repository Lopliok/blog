{
  "name": "nestjs-boilerplate",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "engines": {
    "node": "10.16.3"
  },
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "node dist/src/main.js",
    "start:backup": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "tsc-watch -p tsconfig.build.json --onSuccess \"node dist/src/main.js\"",
    "start:debug": "tsc-watch -p tsconfig.build.json --onSuccess \"node --inspect-brk dist/src/main.js\"",
    "start:prod": "node dist/src/main.js",
    "start:heroku": "node dist/src/main.js",
    "start:dist": "node dist/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/apollo": "^12.2.0",
    "@nestjs/common": "^10.3.10",
    "@nestjs/core": "^10.3.10",
    "@nestjs/devtools-integration": "^0.1.6",
    "@nestjs/graphql": "^12.2.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.10",
    "@nodelib/fs.stat": "^3.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "apollo-server-express": "^3.13.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "fs": "^0.0.1-security",
    "graphql": "^16.9.0",
    "graphql-tools": "^9.0.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.12.0",
    "pg-hstore": "^2.3.4",
    "prisma": "^5.18.0",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^6.0.1",
    "rxjs": "^7.8.1",
    "sequelize": "^6.37.3",
    "sequelize-typescript": "^2.1.6",
    "ts-morph": "^22.0.0",
    "ts-node": "10.9.2"
  },
  "devDependencies": {
    "@nestjs/testing": "^10.3.10",
    "@now/node": "^1.8.5",
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "4.17.21",
    "@types/jest": "29.5.12",
    "@types/node": "22.2.0",
    "@types/passport-jwt": "^4.0.1",
    "@types/sequelize": "^4.28.20",
    "@types/supertest": "6.0.2",
    "jest": "29.7.0",
    "prettier": "3.3.3",
    "supertest": "7.0.0",
    "ts-jest": "29.2.4",
    "tsc-watch": "6.2.0",
    "tsconfig-paths": "4.2.0",
    "tslint": "6.1.3",
    "typescript": "5.5.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
